  "description": "Tiled Utils for Pixi.js - usable with node.js with pixi-shim",
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "lint": "eslint --config eslint.config.js --ignore-pattern eslint.config.js . --fix",
    "format": "prettier *.* --write",
