{
  "name": "tiled-utils",
  "version": "3.1.0",
  "description": "A collection of helpful methods and objects for using Tiled Editor with Pixi.js.",
  "main": "dist/index.js",
  "module": "index.js",
  "scripts": {
    "publish": "npm publish",
    "clean": "rimraf dist",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "lint": "eslint ./source --fix",
    "compile": "node_modules/.bin/babel ./source --out-dir ./es2015 --presets es2015"
  },
  "files": [
    "dist/",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/griest/pexi.git"
  },
  "keywords": [
    "Pixi.js",
    "js",
    "game engine"
  ],
  "author": "Peter Lauck",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/griest/pexi/issues"
  },
  "homepage": "https://gitlab.com/griest/pexi/tree/master/src/tile-utilities#README",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "editions": "^1.3.3"
  },
  "editions": [
    {
      "description": "Source + ESNext + Import",
      "entry": "source/index.js",
      "syntaxes": [
        "javascript",
        "esnext",
        "import",
        "arrows",
        "const",
        "let",
        "destructuring"
      ]
    },
    {
      "description": "Babel Compiled + ES2015 + Require",
      "entry": "es2015/index.js",
      "syntaxes": [
        "javascript",
        "es2015",
        "require"
      ]
    }
  ]  
}
